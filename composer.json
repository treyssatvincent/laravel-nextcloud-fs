{
    "name": "pbmedia/laravel-webdav",
    "type": "library",
    "description": "Laravel WebDAV Filesystem",
    "keywords": [
        "pbmedia",
        "laravel-webdav"
    ],
    "homepage": "https://github.com/pascalbaljetmedia/laravel-webdav",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "info@protone.media",
            "homepage": "https://protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/filesystem": "^6.0 || ^7.0",
        "league/flysystem-webdav": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^4.0",
        "phpunit/phpunit": "~7.0|~8.0"
    },
    "autoload": {
        "psr-4": {
            "Pbmedia\\FilesystemProviders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pbmedia\\FilesystemProviders\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pbmedia\\FilesystemProviders\\WebDAVServiceProvider"
            ]
        }
    }
}